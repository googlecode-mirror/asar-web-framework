#!/usr/bin/env php
<?php
set_include_path(
  realpath(dirname(__FILE__) . '/../core' ) . PATH_SEPARATOR . 
  get_include_path()
);

require_once 'Asar.php';
$cli = Asar_Utility_CLI::instance();
$cli->register(new Asar_Utility_CLI_BaseTasks);

// Load other tasks
if (file_exists('tasks.php')) {
  include_once 'tasks.php';
}
$cli->execute($argv);
?>

